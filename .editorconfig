root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{cpp,h,py,txt,cmake}]
charset = utf-8
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true

[*.{js,json}]
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
